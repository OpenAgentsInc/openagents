# Autopilot uses the Workers AI binding by default.
# If you switch providers, add secrets here and upload via `wrangler secret bulk`.
# OPENAI_API_KEY=

# Enable Sky logging/export paths locally.
# AUTOPILOT_SKY_MODE=1

# Tool policy + HTTP allowlist (default is none).
# AUTOPILOT_TOOL_POLICY=read-only
# AUTOPILOT_TOOL_CHOICE=auto
# AUTOPILOT_TOOL_MAX_CALLS=4
# AUTOPILOT_TOOL_MAX_OUTBOUND_BYTES=200000
# AUTOPILOT_TOOL_ADMIN_SECRET=replace-me
# AUTOPILOT_EXECUTOR_KIND=workers
# AUTOPILOT_TUNNEL_URL=https://tunnel.example.com
# AUTOPILOT_TUNNEL_TOKEN=replace-me
# AUTOPILOT_TUNNEL_TIMEOUT_MS=8000
# AUTOPILOT_TUNNEL_ACCESS_CLIENT_ID=replace-me
# AUTOPILOT_TUNNEL_ACCESS_CLIENT_SECRET=replace-me
# AUTOPILOT_HTTP_ALLOWLIST=example.com,api.example.com
# AUTOPILOT_HTTP_MAX_BYTES=50000
# AUTOPILOT_HTTP_TIMEOUT_MS=8000

# Codex (sandbox auth)
# AUTOPILOT_CODEX_SECRET=replace-me
# AUTOPILOT_CODEX_CORS_ORIGIN=https://openagents.com

# Extensions (Phase 5)
# AUTOPILOT_EXTENSION_ALLOWLIST=sky.echo@0.1.0
# AUTOPILOT_EXTENSION_DEFAULTS=sky.echo@0.1.0
# AUTOPILOT_EXTENSION_CATALOG_URL=https://example.com/extensions.json
# AUTOPILOT_EXTENSION_CATALOG_JSON=[]
# AUTOPILOT_EXTENSION_CATALOG_KEY=extensions/catalog.json
# AUTOPILOT_EXTENSION_ADMIN_SECRET=replace-me
