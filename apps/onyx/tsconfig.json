{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "noImplicitAny": false,
    "module": "esnext",
    "moduleResolution": "node",
    "paths": {
      "@/*": [
        "./app/*"
      ],
      "~/*": [
        "./app/*"
      ],
      "@openagents/ui": [
        "../../packages/ui/src"
      ],
      "@openagents/ui/*": [
        "../../packages/ui/src/*"
      ]
    },
  }
}
