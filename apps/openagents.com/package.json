{
  "name": "@openagentsinc/openagents.com",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "bun run --hot --env-file=../../.env src/index.ts",
    "build": "tsc && bun run build:client && bun run build:sdk && bun run generate:llms-txt",
    "build:client": "bun run src/build-client.ts",
    "build:sdk": "bun run src/build-sdk-browser.ts",
    "typecheck": "tsc --noEmit",
    "generate:llms-txt": "tsx ../../scripts/generate-llms-txt.ts"
  },
  "dependencies": {
    "@effect/platform": "0.85.2",
    "@effect/platform-bun": "^0.70.5",
    "@effect/platform-node": "0.86.5",
    "@noble/curves": "1.2.0",
    "@noble/hashes": "1.3.1",
    "@openagentsinc/ai": "workspace:*",
    "@openagentsinc/nostr": "workspace:*",
    "@openagentsinc/psionic": "workspace:*",
    "@openagentsinc/relay": "workspace:*",
    "@openagentsinc/sdk": "workspace:*",
    "@openagentsinc/ui": "workspace:*",
    "dotenv": "^16.4.7",
    "effect": "3.16.8",
    "elysia": "^1.3.5",
    "mysql2": "^3.11.5"
  },
  "devDependencies": {
    "@types/bun": "^1.2.16",
    "esbuild": "^0.25.5",
    "gray-matter": "^4.0.3"
  }
}
