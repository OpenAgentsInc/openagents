{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "license": "MIT",
  "homepage": "https://github.com/workos/template-convex-tanstack-start-authkit/#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/workos/template-convex-tanstack-start-authkit.git"
  },
  "bugs": {
    "url": "https://github.com/workos/template-convex-tanstack-start-authkit/issues"
  },
  "scripts": {
    "dev": "npm-run-all --parallel dev:frontend dev:backend",
    "dev:frontend": "vite dev",
    "dev:backend": "convex dev",
    "predev": "convex dev --until-success",
    "dashboard": "convex dashboard",
    "build": "vite build",
    "build:prod": "bash -c 'set -a; [ -f .env ] && . ./.env; [ -f .env.production ] && . ./.env.production; set +a; VITE_CONVEX_URL=${VITE_CONVEX_URL:-https://successful-mongoose-647.convex.cloud} vite build'",
    "start": "vite start",
    "deploy": "npm run deploy:convex && bash -c 'set -a; [ -f .env ] && . ./.env; [ -f .env.production ] && . ./.env.production; set +a; VITE_CONVEX_URL=${VITE_CONVEX_URL:-https://successful-mongoose-647.convex.cloud} npm run build && wrangler deploy'",
    "deploy:convex": "bash -c 'set -a; [ -f .env.production ] && . ./.env.production; set +a; npx convex deploy --yes'",
    "cf-typegen": "wrangler types",
    "format": "prettier --write .",
    "lint": "tsc && eslint .  --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "test": "vitest run --reporter=dot",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:auth": "node ./e2e/save-auth-state.mjs"
  },
  "dependencies": {
    "@ai-sdk/openai": "^3.0.19",
    "@assistant-ui/react": "^0.12.1",
    "@assistant-ui/react-ai-sdk": "^1.3.1",
    "@assistant-ui/react-markdown": "^0.12.0",
    "@convex-dev/react-query": "^0.1.0",
    "@nostrify/nostrify": "^0.49.2",
    "@nostrify/react": "^0.2.24",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.4",
    "@radix-ui/react-label": "^2.1.1",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.1",
    "@radix-ui/react-toggle": "^1.1.2",
    "@radix-ui/react-toggle-group": "^1.1.2",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tanstack/react-query": "^5.90.20",
    "@tanstack/react-router": "^1.157.18",
    "@tanstack/react-router-ssr-query": "^1.157.18",
    "@tanstack/react-start": "^1.157.18",
    "@use-gesture/react": "^10.3.1",
    "@workos/authkit-tanstack-react-start": "0.5.0",
    "ai": "^6.0.50",
    "buffer": "^6.0.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "convex": "^1.31.7",
    "date-fns": "^4.1.0",
    "gray-matter": "^4.0.3",
    "lucide-react": "^0.460.0",
    "nostr-tools": "^2.9.0",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-markdown": "^9.0.1",
    "remark-gfm": "^4.0.1",
    "zustand": "^5.0.10"
  },
  "devDependencies": {
    "agent-browser": "^0.9.0",
    "@playwright/test": "^1.50.1",
    "@cloudflare/vite-plugin": "^1.13.8",
    "@convex-dev/eslint-plugin": "^1.1.1",
    "@tailwindcss/postcss": "^4.1.18",
    "@tailwindcss/typography": "^0.5.19",
    "@tanstack/eslint-config": "^0.3.4",
    "@tanstack/react-router-devtools": "^1.157.18",
    "@types/node": "^24.10.9",
    "@types/react": "^19.2.10",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.3",
    "dotenv": "^17.2.3",
    "jsdom": "^24.0.0",
    "npm-run-all2": "^8.0.4",
    "playwright": "^1.50.1",
    "prettier": "^3.8.1",
    "tailwind-merge": "^2.6.0",
    "tw-animate-css": "^1.4.0",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vite-tsconfig-paths": "^6.0.5",
    "vitest": "^2.1.8",
    "wrangler": "^4.61.1"
  }
}
