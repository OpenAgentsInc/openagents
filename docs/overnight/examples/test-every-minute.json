{
  "id": "test-every-minute",
  "workspace_root": "$WORKSPACE",
  "schedule": {
    "expression": "* * * * *",
    "window_start": null,
    "window_end": null,
    "jitter_ms": 0,
    "on_missed": "skip"
  },
  "constraints": {
    "plugged_in": false,
    "wifi_only": false,
    "cpu_max_percentage": null,
    "respect_dnd": false,
    "suspend_if_active": false
  },
  "time_budget_sec": 300,
  "max_concurrent": 1,
  "goals": [
    "Test orchestration sidebar UI with rapid fire cycles",
    "Verify scheduler timing and state updates"
  ],
  "agent_name": "codex",
  "working_directory": "$WORKSPACE",
  "logs_directory": "$WORKSPACE/.openagents/logs/test-runs"
}
