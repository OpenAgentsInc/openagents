{
  "expo": {
    "name": "OpenAgents",
    "slug": "openagents",
    "version": "0.2.1",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "openagents",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.openagents.app",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSCameraUsageDescription": "OpenAgents uses the camera to scan pairing QR codes.",
        "NSLocalNetworkUsageDescription": "OpenAgents discovers and connects to your desktop over the local network or Tailscale.",
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true,
          "NSAllowsLocalNetworking": true
        }
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "./plugins/ios-provisioning-profile",
        {
          "teamId": "HQWSG26L43"
        }
      ],
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#000000"
        }
      ],
      "expo-camera"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "39df2bde-2c42-49ec-9cef-1567231dfe38"
      }
    },
    "owner": "openagents-org",
    "runtimeVersion": "0.2.0",
    "updates": {
      "url": "https://u.expo.dev/39df2bde-2c42-49ec-9cef-1567231dfe38"
    }
  }
}
