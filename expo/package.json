{
  "name": "openagents",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "start:sb": "EXPO_PUBLIC_USE_STORYBOOK=1 expo start",
    "android": "expo run:android",
    "android:sb": "EXPO_PUBLIC_USE_STORYBOOK=1 expo run:android",
    "ios": "expo run:ios",
    "ios:sb": "EXPO_PUBLIC_USE_STORYBOOK=1 expo run:ios",
    "web": "expo start --web",
    "copy:sources": "node ../scripts/copy-sources-to-clipboard.js",
    "typecheck": "tsc -p tsconfig.app.json --noEmit",
    "lint": "expo lint",
    "test": "bun test",
    "build:ios:prod": "eas build --profile production --platform ios",
    "submit:ios": "eas submit -p ios",
    "update:both": "eas update --channel v0.3.0 --environment production --message",
    "update:ios": "eas update --channel v0.3.0 --environment production --platform ios --message",
    "update:android": "eas update --channel v0.3.0 --environment production --platform android --message"
  },
  "dependencies": {
    "@agentclientprotocol/sdk": "^0.5.1",
    "@expo/vector-icons": "^15.0.2",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-navigation/bottom-tabs": "^7.4.0",
    "@react-navigation/elements": "^2.6.3",
    "@react-navigation/native": "^7.1.8",
    "expo": "54.0.20",
    "expo-camera": "~17.0.8",
    "expo-clipboard": "~8.0.7",
    "expo-constants": "~18.0.10",
    "expo-font": "~14.0.9",
    "expo-haptics": "~15.0.7",
    "expo-image": "~3.0.10",
    "expo-linking": "~8.0.8",
    "expo-router": "~6.0.13",
    "expo-splash-screen": "~31.0.10",
    "expo-status-bar": "~3.0.8",
    "expo-symbols": "~1.0.7",
    "expo-system-ui": "~6.0.8",
    "expo-updates": "^29.0.12",
    "expo-web-browser": "~15.0.8",
    "prism-react-renderer": "^2.4.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-drawer-layout": "^4.2.0",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-markdown-display": "^7.0.2",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-web": "~0.21.0",
    "react-native-worklets": "0.5.1",
    "setimmediate": "^1.0.5",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@storybook/addon-ondevice-actions": "^10.0.0",
    "@storybook/addon-ondevice-controls": "^10.0.0",
    "@storybook/react-native": "^10.0.0",
    "@types/react": "~19.1.0",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~10.0.0",
    "typescript": "~5.9.2"
  },
  "private": true
}
