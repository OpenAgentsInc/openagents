{
  "name": "@openagentsinc/desktop",
  "version": "0.1.1-golden-loop-v2-demo",
  "description": "Your agent command center",
  "scripts": {
    "dev": "bun run desktop",
    "build": "bun run build:frontend && bun build --compile src/desktop/main.ts --outfile openagents",
    "build:frontend": "bun build src/mainview/effuse-main.ts --outfile src/mainview/effuse-main.js --target browser --format=iife",
    "start": "bun run desktop",
    "typecheck": "tsc --noEmit -p tsconfig.typecheck.json",
    "test": "bun test",
    "test:container": "bun test src/bench/ src/llm/ src/hud/ src/tbench-hud/ src/schemas/ src/tasks/schema.test.ts src/agent/orchestrator/claude-code-subagent.test.ts src/agent/orchestrator/subagent-router.test.ts",
    "lint": "biome lint .",
    "lint:fix": "biome lint --write .",
    "format": "biome format .",
    "format:check": "biome format --check .",
    "models:generate": "bun scripts/generate-models.ts",
    "prepare": "effect-language-service patch",
    "hi": "bun --bun src/llm/openrouter-cli.ts -- --prompt 'Say hi'",
    "demo:openrouter": "bun --bun src/llm/openrouter-inference-demo.ts",
    "demo:edit": "bun --bun src/tools/edit-demo.ts",
    "demo:agent-tools": "echo \"openrouter tool demo removed\"",
    "tasks:init": "bun src/tasks/cli.ts init",
    "tasks:list": "bun src/tasks/cli.ts list",
    "tasks:ready": "bun src/tasks/cli.ts ready",
    "tasks:next": "bun src/tasks/cli.ts next",
    "tasks:create": "bun src/tasks/cli.ts create",
    "tasks:update": "bun src/tasks/cli.ts update",
    "tasks:close": "bun src/tasks/cli.ts close",
    "tasks:archive": "bun src/tasks/cli.ts archive",
    "tasks:search": "bun src/tasks/cli.ts search",
    "tasks:integrity": "bun scripts/tasks-integrity.ts",
    "healer:scan": "bun src/healer/cli.ts scan",
    "researcher:search": "bun src/researcher/cli.ts search",
    "researcher:url": "bun src/researcher/cli.ts url",
    "researcher:from-reflection": "bun src/researcher/cli.ts from-reflection",
    "researcher:auto": "bun src/researcher/cli.ts auto",
    "researcher:status": "bun src/researcher/cli.ts status",
    "researcher:list": "bun src/researcher/cli.ts list",
    "researcher:synthesize": "bun src/researcher/cli.ts synthesize",
    "mechacoder": "bun src/agent/do-one-task.ts",
    "mechacoder:cc-only": "bun src/agent/do-one-task.ts --cc-only",
    "mechacoder:legacy": "bun src/agent/do-one-task.ts --legacy",
    "mechacoder:overnight": "bun src/agent/overnight.ts",
    "mechacoder:overnight:cc-only": "bun src/agent/overnight.ts --cc-only",
    "mechacoder:overnight:legacy": "bun src/agent/overnight.ts --legacy",
    "mechacoder:parallel": "bun src/agent/overnight-parallel.ts",
    "mechacoder:parallel:cc-only": "bun src/agent/overnight-parallel.ts --cc-only",
    "export-html": "bun src/agent/export-html.ts",
    "session:list": "bun src/sessions/cli.ts list",
    "session:show": "bun src/sessions/cli.ts show",
    "session:search": "bun src/sessions/cli.ts search",
    "session:by-task": "bun src/sessions/cli.ts by-task",
    "usage:summary": "bun src/usage/cli.ts",
    "e2e:server": "bun run e2e/test-server.ts",
    "e2e:test": "bunx playwright test --config e2e/playwright.config.ts",
    "e2e:test:headed": "HEADED=1 bunx playwright test --config e2e/playwright.config.ts --headed",
    "e2e:test:slow": "HEADED=1 SLOWMO=500 bunx playwright test --config e2e/playwright.config.ts --headed",
    "e2e:test:debug": "PWDEBUG=1 HEADED=1 SLOWMO=500 bunx playwright test --config e2e/playwright.config.ts --debug",
    "e2e:ui": "bunx playwright test --config e2e/playwright.config.ts --ui",
    "e2e:report": "bunx playwright show-report",
    "e2e:update-snapshots": "bunx playwright test --config e2e/playwright.config.ts --update-snapshots",
    "e2e:desktop": "bunx playwright test --config e2e/playwright.desktop.config.ts",
    "e2e:desktop:headed": "HEADED=1 bunx playwright test --config e2e/playwright.desktop.config.ts --headed",
    "health": "bun src/health/cli.ts",
    "health:report": "bun src/health/report-cli.ts",
    "deps:audit": "bun src/deps/audit-cli.ts",
    "deps:update": "bun src/deps/update-cli.ts",
    "desktop": "bun src/desktop/main.ts",
    "desktop:debug": "bun --inspect src/desktop/main.ts",
    "new": "bun src/desktop/main.ts --new",
    "session:export-html": "bun src/sessions/cli.ts export-html",
    "build:health": "bun scripts/generate-build-health.ts",
    "tbench:local": "bun src/cli/tbench-local.ts",
    "tbench:fm-mini": "bun src/cli/tbench-local.ts --suite tasks/fm-mini-suite.json --output .openagents/tb-runs/fm-mini-$(date +%Y%m%d-%H%M%S) --model fm",
    "tbench:fm-mini:iterate": "bun src/cli/tbench-iterate.ts --suite tasks/fm-mini-suite.json --max-tasks 10 --model fm",
    "bridge": "./bin/foundation-bridge",
    "bridge:build": "cd swift/foundation-bridge && ./build.sh",
    "hillclimber": "bun src/hillclimber/cli.ts",
    "hillclimber:overnight": "bun src/hillclimber/cli.ts --max-runs 500 --sleep 30000",
    "hillclimber:stats": "bun src/hillclimber/cli.ts --stats",
    "hillclimber:export": "bun src/hillclimber/cli.ts --export",
    "test-gen:compare": "bun src/hillclimber/test-gen-compare.ts",
    "hf:download": "bun src/huggingface/cli.ts download",
    "hf:list": "bun src/huggingface/cli.ts list",
    "hf:info": "bun src/huggingface/cli.ts info",
    "hf:delete": "bun src/huggingface/cli.ts delete",
    "hf:count": "bun src/huggingface/cli.ts count",
    "hf:sample": "bun src/huggingface/cli.ts sample"
  },
  "dependencies": {
    "@anthropic-ai/claude-agent-sdk": "0.1.56",
    "@duckdb/duckdb-wasm": "1.30.0",
    "@effect/cli": "0.72.1",
    "@effect/platform": "0.93.6",
    "@effect/platform-bun": "0.86.0",
    "@huggingface/hub": "2.7.1",
    "@openrouter/sdk": "0.1.27",
    "@opentelemetry/api": "1.9.0",
    "@opentelemetry/exporter-metrics-otlp-http": "0.52.1",
    "@opentelemetry/exporter-trace-otlp-http": "0.52.1",
    "@opentelemetry/sdk-node": "0.52.1",
    "apache-arrow": "21.1.0",
    "diff": "8.0.2",
    "effect": "3.19.8",
    "parquet-wasm": "0.7.1",
    "parquetjs-lite": "0.8.7",
    "smol-toml": "1.5.2",
    "three": "^0.181.2",
    "webview-bun": "2.4.0",
    "zod": "3.24.4"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.8",
    "@effect/language-service": "0.58.0",
    "@playwright/test": "^1.57.0",
    "@types/bun": "1.3.3",
    "@types/three": "^0.181.0",
    "happy-dom": "^20.0.11",
    "typescript": "5.9.3"
  }
}
