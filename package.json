    "@effect/platform-bun": "0.86.0",
    "@openrouter/sdk": "0.1.27",
    "diff": "8.0.2",
    "effect": "3.19.8",
    "electrobun": "0.1.12"
  },
  "devDependencies": {
    "@effect/language-service": "0.58.0",
    "@types/bun": "1.3.3",
    "typescript": "5.9.3"
  }
