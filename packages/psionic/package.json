    "dompurify": "^3.2.6",
    "drizzle-orm": "^0.44.2",
    "effect": "3.16.8",
    "gray-matter": "^4.0.3",
    "jsdom": "^26.1.0",
    "markdown-it": "^14.1.0",
    "shiki": "^3.6.0",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@openagentsinc/autotest": "workspace:*",
    "@types/bun": "^1.2.16",
    "@types/dompurify": "^3.2.0",
    "@types/jsdom": "^21.1.7",
    "@types/markdown-it": "^14.1.2",
    "@types/uuid": "^10.0.0",
    "elysia": "^1.3.5"
  }
