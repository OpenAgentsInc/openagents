{
  "name": "@openagentsinc/psionic",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./browser": {
      "types": "./src/browser.ts",
      "import": "./src/browser.ts",
      "default": "./src/browser.ts"
    },
    "./types": {
      "types": "./src/types/index.ts",
      "import": "./src/types/index.ts",
      "default": "./src/types/index.ts"
    }
  },
  "scripts": {
    "dev": "bun run --hot examples/hello-world.ts",
    "typecheck": "tsc --noEmit",
    "build": "tsc"
  },
  "dependencies": {
    "@effect/platform": "0.85.2",
    "@effect/platform-bun": "0.70.5",
    "@electric-sql/pglite": "^0.3.3",
