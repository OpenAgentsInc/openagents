{
  // Cloudflare Worker configuration for OpenAgents Pylon
  "name": "openagents-pylon",
  "compatibility_date": "2025-06-14",
  
  // Build command
  "build": {
    "command": "pnpm build"
  },
  
  // Static assets configuration for Vite build output
  "assets": {
    "directory": "./dist"
  },
  
  // Custom domain configuration
  "routes": [
    {
      "pattern": "pylon.openagents.com",
      "custom_domain": true
    }
  ]
}