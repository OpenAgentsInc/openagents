# PlanetScale Database Configuration
DATABASE_HOST=aws.connect.psdb.cloud
DATABASE_USERNAME=your_username_here
DATABASE_PASSWORD=your_password_here
DATABASE_NAME=openagents_relay

# Relay Configuration
RELAY_MAX_CONNECTIONS=1000
RELAY_RATE_LIMIT_ENABLED=false
RELAY_RATE_LIMIT_PER_MINUTE=60
RELAY_ENABLE_METRICS=true

# CORS Configuration  
RELAY_ENABLE_CORS=true
RELAY_ALLOWED_ORIGINS=*