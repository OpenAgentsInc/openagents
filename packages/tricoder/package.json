{
  "name": "tricoder",
  "version": "0.3.0",
  "type": "module",
  "author": "OpenAgents, Inc.",
  "license": "Apache-2.0",
  "description": "DEPRECATED: OpenAgents Tricoder bridge (v0.2.x only, use Swift v0.3+ instead)",
  "deprecated": "This package is no longer supported. OpenAgents v0.3+ uses native Swift architecture. Last working version was 0.2.5. See README for migration info.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OpenAgentsInc/openagents.git",
    "directory": "packages/tricoder"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "default": "./dist/types/index.js"
    }
  },
  "bin": {
    "tricoder": "dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "prepublishOnly": "npm run build && chmod +x dist/index.js",
    "dev": "tsx src/index.ts",
    "test": "node --import tsx --test src/**/*.test.ts"
  },
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "adm-zip": "^0.5.16",
    "chalk": "^5.3.0",
    "qrcode": "^1.5.3",
    "qrcode-terminal": "^0.12.0",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@types/adm-zip": "^0.5.7",
    "@types/node": "^22.8.6",
    "@types/qrcode": "^1.5.5",
    "@types/qrcode-terminal": "^0.12.2",
    "@types/ws": "^8.5.12",
    "tsx": "^4.19.1",
    "typescript": "^5.9.3"
  }
}
