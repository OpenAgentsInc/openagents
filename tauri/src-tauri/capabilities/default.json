{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main"],
  "webviews": ["*"],
  "remote": {
    "urls": [
      "http://localhost:19006/*",
      "http://127.0.0.1:19006/*",
      "http://0.0.0.0:19006/*",
      "http://192.168.*.*:19006/*",
      "http://10.*.*.*:19006/*",
      "http://172.*.*.*:19006/*"
    ]
  },
  "permissions": [
    "core:default",
    "core:app:default",
    "core:event:default",
    "core:window:default",
    "opener:default"
  ]
}
